<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <SignAssembly>false</SignAssembly>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Title>GameLauncherCore</Title>
    <Version>1.0.1</Version>
    <Authors>Carlos Arturo Rodriguez Silva</Authors>
    <Description>Create patches for your game or app</Description>
    <PackageProjectUrl>https://game-launcher.net/</PackageProjectUrl>
    <PackageTags>Launcher, Patcher, Updater</PackageTags>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Copyright>https://game-launcher.net/</Copyright>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.DotNet.UpgradeAssistant.Extensions.Default.Analyzers" Version="0.4.410601">
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
  </ItemGroup>
</Project>